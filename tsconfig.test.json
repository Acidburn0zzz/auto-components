{
  "compilerOptions": {
    "target": "ES2018",
    "lib": ["dom", "esnext"],
    "module": "esnext",
    "moduleResolution": "node",
    "skipLibCheck": true,
    "esModuleInterop": true,
    "strict": true,
    "noImplicitAny": false,
    "noFallthroughCasesInSwitch": true,
    "noImplicitReturns": true,
    "stripInternal": true,
    "noUnusedLocals": true
  },
  "include": ["test"]
}
